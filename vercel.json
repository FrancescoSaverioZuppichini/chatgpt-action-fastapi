{
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      }
    ],
    "functions": {
        "api/index.py": {
          "includeFiles": [
            "lib/**"
          ]
        }
      },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }